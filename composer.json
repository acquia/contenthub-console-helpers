{
    "name": "acquia/console-helpers",
    "description": "A package providing Helper Libraries for the CommonConsole command line interface.",
    "type": "project",
    "require": {
        "ext-json": "*",
        "eclipsegc/common-console": "dev-LCH-4554"
    },
    "autoload": {
        "psr-4": {
            "Acquia\\Console\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acquia\\Console\\Helpers\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
